<?xml version="1.0" encoding="utf-8"?>
<test-group name="langlib decimal test">
    <description>tests for langlib decimal</description>
    <test name="decimallib-test">
        <description>decimal test</description>
        <source>
            <file>test-src/decimallib_test.bal</file>
        </source>
        <steps>
            <run>
                <callFunction name="testMaxAsMethodInvok">
                    <parameters>
                        <decimal>0</decimal>
                        <arrayValue>
                            <decimal>0</decimal>
                        </arrayValue>
                    </parameters>
                    <assert>
                        <return>
                            <decimal>0</decimal>
                        </return>
                    </assert>
                    <parameters>
                        <decimal>0.0</decimal>
                        <arrayValue>
                            <decimal>0</decimal>
                        </arrayValue>
                    </parameters>
                    <assert>
                        <return>
                            <decimal>0</decimal>
                        </return>
                    </assert>
                    <parameters>
                        <decimal>0</decimal>
                        <arrayValue>
                            <decimal>1</decimal>
                        </arrayValue>
                    </parameters>
                    <assert>
                        <return>
                            <decimal>1</decimal>
                        </return>
                    </assert>
                    <parameters>
                        <decimal>-1</decimal>
                        <arrayValue>
                            <decimal>0</decimal>
                        </arrayValue>
                    </parameters>
                    <assert>
                        <return>
                            <decimal>0</decimal>
                        </return>
                    </assert>
                    <parameters>
                        <decimal>-0</decimal>
                        <arrayValue>
                            <decimal>0</decimal>
                        </arrayValue>
                    </parameters>
                    <assert>
                        <return>
                            <decimal>0</decimal>
                        </return>
                    </assert>
                    <parameters>
                        <decimal>5</decimal>
                        <arrayValue>
                            <decimal>0,2,-2</decimal>
                        </arrayValue>
                    </parameters>
                    <assert>
                        <return>
                            <decimal>5</decimal>
                        </return>
                    </assert>
                    <parameters>
                        <decimal>511111111111199999999999222222222.2222222</decimal>
                        <arrayValue>
                            <decimal>0</decimal>
                        </arrayValue>
                    </parameters>
                    <assert>
                        <return>
                            <decimal>511111111111199999999999222222222.2222222</decimal>
                        </return>
                    </assert>
                </callFunction>
